{
  "name": "portfolio-os",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "react-router dev --host",
    "debug": "NODE_OPTIONS='--inspect-brk' npm run dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client --d1 DB=5c98e7f0-f7f6-47b9-af4c-e7ea32bd52b8 --live-reload --persist-to=./wrangler",
    "typecheck": "react-router typegen && tsc --watch",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "d1-migrate:local": "wrangler d1 execute portfolio-d1 --local --file=./schema.sql",
    "d1-migrate": "wrangler d1 execute portfolio-d1 --remote --file=./schema.sql"
  },
  "dependencies": {
    "@react-router/cloudflare": "^7.2.0",
    "@react-router/fs-routes": "^7.1.5",
    "esbuild": "^0.25.1",
    "isbot": "^5.1.25",
    "react": "^19.1.0",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.4.6",
    "react-google-recaptcha-enterprise": "^1.0.3",
    "react-router": "^7.1.5",
    "react-simple-typewriter": "^5.0.1",
    "resend": "^4.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250204.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.24.0",
    "@react-router/dev": "^7.4.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.13.4",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.0.4",
    "@types/react-google-recaptcha-enterprise": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.1",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "remark-frontmatter": "^5.0.0",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

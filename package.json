{
  "name": "portfolio-os",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "react-router dev --host",
    "debug": "NODE_OPTIONS='--inspect-brk' npm run dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client --d1 DB=5c98e7f0-f7f6-47b9-af4c-e7ea32bd52b8 --live-reload --persist-to=./wrangler",
    "typecheck": "react-router typegen && tsc --watch",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "d1-migrate:local": "wrangler d1 execute portfolio-d1 --local --file=./schema.sql",
    "d1-migrate": "wrangler d1 execute portfolio-d1 --remote --file=./schema.sql"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@react-router/cloudflare": "^7.1.1",
    "@react-router/fs-routes": "^7.1.1",
    "esbuild": "^0.24.2",
    "isbot": "^5.1.19",
    "mdx-bundler": "^10.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.4.6",
    "react-router": "^7.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241224.0",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@react-router/dev": "^7.1.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/mdx": "^2.0.13",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.15.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^3.91.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
